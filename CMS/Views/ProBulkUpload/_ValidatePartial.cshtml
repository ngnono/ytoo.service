@model IEnumerable<Yintai.Hangzhou.Data.Models.ProductValidateResult>
@using Yintai.Hangzhou.Cms.WebSiteV1.Util
@if( Model.Count() >0) {
    <tr>
        <th>
            商品编码
        </th>
        <th>验证状态</th>
        <th>验证结果</th>
    </tr>

    foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ItemCode)
        </td>
         <td>
           <p>@item.ResultStatus.ToFriendlyString()</p> 
        </td>
         <td>
            @Html.DisplayFor(modelItem => item.ValidateResult)
        </td>
       
    </tr>
    }
} else {
    <tr> <td>
        <h4>验证通过！</h4>
         </td>
    </tr>
}


