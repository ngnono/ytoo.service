@using Yintai.Hangzhou.Model.Enums
@using Yintai.Hangzhou.WebSupport.Helper
@model Yintai.Hangzhou.Cms.WebSiteV1.Models.CategoryViewModel
@{
    ViewBag.Title = "分类映射";  
}
@section PageBread
{
    <ul class="breadcrumb">
        <li>@Html.ActionLink2("配置管理", "Index","Configuration")<span class="divider">/</span></li>
        <li>@Html.ActionLink2("基础分类列表", "List")<span class="divider">/</span></li>
        <li class="active">@ViewBag.Title</li>
    </ul>
}

<h2>@ViewBag.Title</h2>
@using (Html.BeginForm("Map",null, FormMethod.Post))
{
     @Html.EditorForModel()
   <div class="form-actions">
        <input type="submit" value="保存" class="btn btn-primary" />
        @Html.ActionLink2("返回列表", "List", null, new { @class = "btn" })
    </div>
}
